  },
  "homepage": "https://github.com/akdasa-studios/framework#readme",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.4.1",
    "@stryker-mutator/jest-runner": "^6.4.1",
    "@stryker-mutator/typescript-checker": "^6.4.1",
    "@types/jest": "^29.4.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "resolve-tspaths": "^0.8.8",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack-cli": "^5.0.1",
    "webpack": "^5.76.2",
    "@microsoft/api-extractor": "^7.34.4"
  }
